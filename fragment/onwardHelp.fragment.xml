<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m">
	<Dialog title="Choose Onward Del Point" items="{OnwardPoints>/}" contentHeight="auto">
        <List items="{OnwardPoints>/}">
            <StandardListItem title="{OnwardPoints>CustomerNumber}" description="{OnwardPoints>Name}" press=".orders.addOnwardPoint" type="Active"/>
        </List>
        <beginButton>
            <Button text="Remove Onward Point" press=".orders.removeOnwardPoint"/>
        </beginButton>
        <endButton>
            <Button text="Cancel" press=".orders.closeOnwardHelp"/>
        </endButton>
    </Dialog>
</core:FragmentDefinition>